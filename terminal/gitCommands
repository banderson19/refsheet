echo "# cheatsheet" >> README.md
git init
git add README.md
git commit -m "first commit"
git branch -M main
git remote add origin git@github.com:banderson19/cheatsheet.git
git push -u origin main


##
git checkout -b "branch name"
git branch --> shows what branch were currently working in

rm -rf build --> deletes build folder
git push <remote>(origin) --delete <branch>. --> deletes remote branch

git push origin feature/name
git checkout develop
git merge feature/name
